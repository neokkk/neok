{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "indent": [true, "spaces", 2],
    "jsRules": {},
    "no-console": [true],
    "rules": {
      "quotemark": [true, "single", "jsx-double"],
      "interface-name": [false],
      "jsx-no-multiline-js": [false]
    },
    "rulesDirectory": [],
    "semicolon": [
      true,
      "always"
    ]
}
