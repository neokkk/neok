{
    "extends": ["eslint:recommended"],
    "indent": [true, "spaces", 2],
    "no-console": [true],
    "rules": {
      "quotemark": [true, "single", "jsx-double"],
      "interface-name": [false],
      "jsx-no-multiline-js": [false]
    },
    "rulesDirectory": [],
    "semicolon": [
      true,
      "always"
    ]
}
